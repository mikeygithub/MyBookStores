<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="user" namespace="/" extends="struts-default">
		<action name="UserAction_*" method="{1}" class="com.bookstore.action.UserAction">
			<result name="Register">WEB-INF/jsp/Register.jsp</result>
		</action>
	</package>
	<package name="reemail" namespace="/" extends="struts-default">
		<action name="LoginAction_*" method="{1}" class="com.bookstore.action.LoginAction"></action>
	</package>
</struts>
