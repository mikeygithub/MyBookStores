<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="book" namespace="/" extends="struts-default">
		<action name="BookAction_*" method="{1}" class="com.bookstore.action.BookAction">
			<result name="BookDetailsUI">WEB-INF/jsp/BookDetails.jsp</result>
			<result name="test">WEB-INF/jsp/ajax.jsp</result>
			<!-- <result name="index" type="redirect">index.jsp</result> -->
			<result name="index">WEB-INF/jsp/index.jsp</result>
			<result name="TypeBookUI">/WEB-INF/jsp/oneTypeBook.jsp</result>
			<result name="getrecentlyUI">WEB-INF/jsp/recentlylook.jsp</result>
		</action>
	</package>
</struts>
