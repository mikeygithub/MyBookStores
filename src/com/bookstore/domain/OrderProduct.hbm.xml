<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bookstore.domain.OrderProduct">
		<id name="opId" length="10">
			<generator class="increment"></generator>
		</id>
		<property name="opNum"></property>
		<property name="opBook"></property>
		<many-to-one name="opOrder" column="oId" class="com.bookstore.domain.Orders"></many-to-one>
	</class>
</hibernate-mapping>