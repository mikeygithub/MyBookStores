<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bookstore.domain.BookType">
		<id name="typeId" length="50">
			<generator class="increment"></generator>
		</id>
		<property name="typeName" length="50" not-null="true"></property>
		<set name="allBook" cascade="all">
			<key>
				<column name="typeId"></column>
			</key>
			<one-to-many class="com.bookstore.domain.Book"/>
		</set>
	</class>

</hibernate-mapping>